{
  "name": "recursiveintelligence-website",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@11.6.2",
  "scripts": {
    "dev": "concurrently --kill-others \"contentlayer2 dev\" \"next dev --turbopack\"",
    "build": "node scripts/build-knowledge-base.mjs && contentlayer2 build && next build",
    "start": "next start",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.996.0",
    "@aws-sdk/client-dynamodb": "^3.996.0",
    "@aws-sdk/lib-dynamodb": "^3.996.0",
    "@react-pdf/renderer": "^4.3.2",
    "@tailwindcss/typography": "^0.5.19",
    "@types/js-yaml": "^4.0.9",
    "contentlayer2": "^0.5.8",
    "js-yaml": "^4.1.1",
    "mdx-bundler": "^10.1.1",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "stripe": "^20.3.1"
  },
  "overrides": {
    "minimatch": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^10.0.2",
    "eslint-config-next": "^16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
